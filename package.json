{
	"name": "glory",
	"version": "2.0.1",
	"author": "Hugo Sum",
	"license": "MIT",
	"type": "commonjs",
	"files": [
		"dist/"
	],
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"typings": "dist/esm/type.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/winston0410/glory.git"
	},
	"scripts": {
		"build": "npx rollup -c",
		"dev": "npx rollup -c -w",
		"test": "npx jest",
		"test:coverage": "npx jest --coverage",
		"test:watch": "npx jest --watch",
		"prepublish": "yarn build",
		"analyze": "npx deoptigate -r esm src/preset.js",
		"size": "npx size-limit",
		"size:analyze": "npx size-limit --why"
	},
	"dependencies": {
		"@blackblock/joli-string": "^1.1.0",
		"css-in-js-utils": "^3.1.0",
		"inline-style-prefixer": "^6.0.0",
		"is-in-browser": "^2.0.0",
		"safe-is-obj": "^1.0.1"
	},
	"devDependencies": {
		"@babel/core": "7.13.13",
		"@babel/preset-env": "7.13.12",
		"@babel/preset-react": "7.13.13",
		"@babel/preset-typescript": "7.13.0",
		"@jest-runner/electron": "3.0.1",
		"@rollup/plugin-commonjs": "17.1.0",
		"@rollup/plugin-node-resolve": "11.2.1",
		"@size-limit/preset-small-lib": "4.10.1",
		"@types/jest": "26.0.22",
		"@typescript-eslint/eslint-plugin": "4.19.0",
		"@typescript-eslint/parser": "4.19.0",
		"babel-jest": "26.6.3",
		"benchmark": "2.1.4",
		"chai": "4.3.4",
		"cross-blob": "2.0.0",
		"deoptigate": "0.5.0",
		"esbuild": "0.10.1",
		"eslint": "7.22.0",
		"eslint-config-prettier": "7.2.0",
		"eslint-config-standard": "16.0.2",
		"eslint-plugin-chai-expect": "2.2.0",
		"eslint-plugin-chai-friendly": "0.6.0",
		"eslint-plugin-const-immutable": "2.1.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-mocha": "8.1.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-promise": "4.3.1",
		"eslint-plugin-standard": "5.0.0",
		"esm": "3.2.25",
		"husky": "4.3.8",
		"jest": "26.6.3",
		"lint-staged": "10.5.4",
		"nyc": "15.1.0",
		"ramda": "0.27.1",
		"react": "17.0.2",
		"react-is": "17.0.2",
		"react-test-renderer": "17.0.2",
		"rollup": "2.42.4",
		"rollup-plugin-multi-input": "1.2.0",
		"rollup-plugin-typescript2": "0.30.0",
		"size-limit": "4.10.1",
		"tiny-worker": "2.3.0",
		"tslib": "2.1.0",
		"typescript": "4.2.3",
		"url-polyfill": "1.1.12",
		"workerpool": "6.1.2"
	},
	"lint-staged": {
		"**/*.js": [
			"npx prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "npx lint-staged",
			"pre-push": "yarn test"
