{
	"name": "camouflage",
	"version": "0.1.0",
	"author": "Hugo Sum",
	"license": "MIT",
	"type": "commonjs",
	"files": [
		"dist/"
	],
	"scripts": {
		"build": "npx rollup -c",
		"test": "npx jest",
		"test:coverage": "npx jest --coverage",
		"test:watch": "npx jest --watch",
		"prepublish": "yarn build",
		"analyze": "npx deoptigate -r esm src/preset.js"
	},
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"default": "./dist/esm/index.js"
		},
		"./jsx": {
			"require": "./dist/cjs/jsx.js",
			"default": "./dist/esm/jsx.js"
		},
		"./rule": {
			"require": "./dist/cjs/rule.js",
			"default": "./dist/esm/rule.js"
		},
		"./prefixer": {
			"require": "./dist/cjs/prefixer.js",
			"default": "./dist/esm/prefixer.js"
		},
		"./hydration": {
			"require": "./dist/cjs/hydration.js",
			"default": "./dist/esm/hydration.js"
		}
	},
	"dependencies": {
		"@blackblock/joli-string": "^1.1.0",
		"browser-or-node": "^1.3.0",
		"comlink": "^4.3.0",
		"css-in-js-utils": "^3.1.0",
		"greenlet": "^1.1.0",
		"inline-style-prefixer": "^6.0.0",
		"rambda": "^6.5.3",
		"safe-is-obj": "^1.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-transform-modules-commonjs": "^7.12.1",
		"@jest-runner/electron": "^3.0.0",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.1.0",
		"babel-jest": "^26.6.3",
		"benchmark": "^2.1.4",
		"chai": "^4.2.0",
		"cross-blob": "^2.0.0",
		"deoptigate": "^0.5.0",
		"esbuild": "^0.8.34",
		"eslint": "^7.18.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-config-standard": "^16.0.2",
		"eslint-plugin-chai-expect": "^2.2.0",
		"eslint-plugin-chai-friendly": "^0.6.0",
		"eslint-plugin-const-immutable": "^2.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-mocha": "^8.0.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^5.0.0",
		"esm": "^3.2.25",
		"husky": "^4.3.8",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.3",
		"nyc": "^15.1.0",
		"ramda": "^0.27.1",
		"react": "^17.0.1",
		"rollup": "^2.38.0",
		"rollup-plugin-multi-input": "^1.2.0",
		"tiny-worker": "^2.3.0",
		"url-polyfill": "^1.1.12",
		"workerpool": "^6.0.4"
	},
	"lint-staged": {
		"**/*.js": [
			"npx prettier --write"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "npx lint-staged",
			"pre-push": "yarn test"
		}
	},
	"keywords": [
		"css",
		"style",
		"styles",
		"pico",
		"nano",
		"lite",
		"react",
		"js",
		"in-js",
		"css-in-js",
		"styled",
		"component",
		"styled-components",
		"jsxstyle",
		"rule",
		"stylesheet"
	]
}
